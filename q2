#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

int main() {
    int n;
    cout << "Enter number of nodes: ";
    cin >> n;

    if (n <= 0) { cout << "List is empty.\n"; return 0; }

    Node* tail = NULL; // we'll keep tail; head = tail->next
    // Build CLL by inserting at end
    for (int i = 0; i < n; i++) {
        int x; cin >> x;
        Node* p = new Node;
        p->data = x;
        if (tail == NULL) {
            tail = p;
            tail->next = tail;     // first node points to itself
        } else {
            p->next = tail->next;  // new->next = head
            tail->next = p;        // old tail -> new
            tail = p;              // move tail
        }
    }

    Node* head = tail->next;

    // Display all nodes once
    Node* cur = head;
    do {
        cout << cur->data << " ";
        cur = cur->next;
    } while (cur != head);

    // Repeat head value at the end
    cout << head->data << "\n";

    return 0;
}
